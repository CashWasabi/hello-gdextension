linux.debug.x86_64 = "libhello-godot-cpp.linux.template_debug.x86_64.so"
linux.release.x86_64 = "libhello-godot-cpp.linux.template_release.x86_64.so"
linux.debug.arm64 = "libhello-godot-cpp.linux.template_debug.arm64.so"
linux.release.arm64 = "libhello-godot-cpp.linux.template_release.arm64.so"
linux.debug.rv64 = "libhello-godot-cpp.linux.template_debug.rv64.so"
