[configuration]
entry_symbol = "hello_cpp_extension_entry"

[libraries]
macos.debug = "libhello-godot-cpp.macos.template_debug.framework"
macos.release = "libhello-godot-cpp.macos.template_release.framework"
windows.debug.x86_32 = "libhello-godot-cpp.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "libhello-godot-cpp.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "libhello-godot-cpp.windows.template_debug.x86_64.dll"
